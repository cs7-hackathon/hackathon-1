{
  "name": "Tab Fever",
  "description": "Notify the user when they've left tabs open for too long",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "notifications",
    "tabs",
    "alarms"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page red",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
